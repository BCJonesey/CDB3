<!DOCTYPE html>
<html>
<head>
  <title>Cdb3</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

<% if flash[:error] %>
  <p class="error_message"><%= flash[:error] %></p>
<% end %>
<% if flash[:notice] %>
  <p class="notice_message"><%= flash[:notice] %></p>
<% end %>

<%= yield %>

</body>
</html>
